<div style="width: 260px;">
  <div class="bgc-cornflower-lilac padding-vertical-2 text-center">
    <a href="/">
      <img src="{{ 'logo.svg' | asset_url }}" alt="logo" style="height: 215px;">
    </a>
  </div>

  <ul class="menu vertical">
    {% for link in linklists.main-menu.links %}
      <li>
        <a href="{{ link.url }}">{{ link.title }}</a>
      </li>
    {% endfor %}
  </ul>
</div>